  },
  "scripts": {
    "build": "node --harmony script/build",
    "build-watch": "node --harmony script/build -- --watch",
    "test": "cross-env NODE_ENV=development nyc mocha --harmony --recursive --require script/test && cross-env NODE_ENV=production nyc --no-clean mocha --harmony --recursive --require script/test",
    "test-watch-dev": "cross-env NODE_ENV=development mocha --harmony --recursive --require script/test --watch",
    "test-watch-prod": "cross-env NODE_ENV=production mocha --harmony --recursive --require script/test --watch",
    "test-debug-dev": "cross-env NODE_ENV=development mocha --harmony --recursive --require script/test",
    "test-debug-prod": "cross-env NODE_ENV=production mocha --harmony --recursive --require script/test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "temp-directory": "./coverage/.nyc_output",
    "exclude": [
      "script/*",
      "test/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chokidar": "^1.7.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.2.0",
    "jsdom": "^11.6.2",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.3.12"
  }
}
