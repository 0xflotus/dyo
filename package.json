{
	"name": "dyo",
	"description": "Dyo is a JavaScript library for building user interfaces",
	"version": "1.0.0",
	"homepage": "https://github.com/thysultan/dio.js",
	"license": "MIT",
	"author": {
		"name": "Sultan Tarimo",
		"email": "sultantarimo@me.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thysultan/dio.js"
	},
	"bugs": {
		"url": "https://github.com/thysultan/dio.js/issues"
	},
	"devDependencies": {
		"dyo": "./",
		"esm": "3.0.84",
		"nyc": "13.1.0",
		"chai": "4.2.0",
		"mocha": "5.2.0",
		"jsdom": "12.2.0",
		"eslint": "5.9.0",
		"coveralls": "3.0.2",
		"cross-env": "5.2.0",
		"rollup-plugin-terser": "3.0.0",
		"rollup": "0.67.3"
	},
	"scripts": {
		"lint": "eslint ./src",
		"build": "rollup --config scripts/build.config.js",
		"start": "npm run build -- --watch",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"prepare": "npm run build",
		"pretest": "npm run build && npm run lint",
		"test": "nyc npm run test-all",
		"test-all": "npm run test-dev && npm run test-prod",
		"test-dev": "cross-env NODE_ENV=development npm run test-spec",
		"test-prod": "cross-env NODE_ENV=production npm run test-spec",
		"test-spec": "mocha --harmony --require esm scripts/tests.config.js --recursive tests"
	},
	"nyc": {
		"temp-dir": "./coverage/.nyc_output",
		"exclude": [
			"scripts",
			"tests"
		],
		"reporter": [
			"lcov",
			"text"
		]
	},
	"esm": {
		"cache": false
	},
	"files": [
		"dist",
		"pkg",
		"src"
	],
	"main": "index.js",
	"module": "./dist/dyo.esm.js",
	"browser": "./dist/dyo.umd.js",
	"unpkg": "./dist/dyo.umd.js",
	"jsdelivr": "./dist/dyo.umd.js"
}
