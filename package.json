    "dist/dio.cjs.production.js",
    "dist/dio.cjs.production.min.js",
    "dist/dio.cjs.production.min.js.map",
    "dist/dio.esm.development.js",
    "dist/dio.esm.development.min.js",
    "dist/dio.esm.development.min.js.map",
    "dist/dio.esm.production.js",
    "dist/dio.esm.production.min.js",
    "dist/dio.esm.production.min.js.map",
    "dist/dio.umd.development.js",
    "dist/dio.umd.development.min.js",
    "dist/dio.umd.development.min.js.map",
    "dist/dio.umd.production.js",
