{
	"name": "dyo",
	"description": "A Library For Building User Interfaces.",
	"version": "9.0.0",
	"homepage": "https://github.com/thysultan/dio.js",
	"license": "MIT",
	"author": {
		"name": "Sultan Tarimo",
		"email": "sultantarimo@me.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thysultan/dio.js"
	},
	"bugs": {
		"url": "https://github.com/thysultan/dio.js/issues"
	},
	"devDependencies": {
		"dyo": "./",
		"esm": "3.0.84",
		"eslint": "5.9.0",
		"rollup": "0.67.1",
		"jsdom": "12.2.0",
		"nyc": "13.1.0",
		"chai": "4.2.0",
		"mocha": "5.2.0",
		"coveralls": "3.0.2",
		"cross-env": "5.2.0"
	},
	"scripts": {
		"lint": "eslint ./src",
		"build": "rollup --config scripts/build.config.js",
		"spec": "",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"start": "npm run build -- --watch",
		"prepare": "npm run build",
		"pretest": "npm run build",
		"test": "nyc npm run test-all",
		"test-all": "npm run test-dev && npm run test-prod",
		"test-dev": "cross-env NODE_ENV=development npm run test-spec",
		"test-prod": "cross-env NODE_ENV=production npm run test-spec",
		"test-spec": "mocha --harmony --require esm scripts/tests.config.js --recursive tests"
	},
	"nyc": {
		"temp-directory": "./coverage/.nyc_output",
		"exclude": [
			"scripts",
			"tests"
		],
		"reporter": [
			"lcov",
			"text"
		]
	},
	"main": "./index.js",
	"module": "dist/dio.es.js",
	"types": "",
	"files": [
		"dist",
		"src",
		"pkg"
	],
	"unpkg": "",
	"jsdelivr": ""
}
