{
	"name": "dyo",
	"description": "A Library For Building User Interfaces.",
	"version": "9.0.0",
	"homepage": "https://github.com/thysultan/dio.js",
	"license": "MIT",
	"author": {
		"name": "Sultan Tarimo",
		"email": "sultantarimo@me.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thysultan/dio.js"
	},
	"bugs": {
		"url": "https://github.com/thysultan/dio.js/issues"
	},
	"devDependencies": {
		"dyo": "./",
		"esm": "3.0.84",
		"eslint": "5.6.1",
		"rollup": "0.66.6",
		"jsdom": "12.2.0",
		"nyc": "13.0.1",
		"chai": "4.2.0",
		"mocha": "5.2.0",
		"coveralls": "3.0.2",
		"cross-env": "5.2.0"
	},
	"scripts": {
		"lint": "eslint ./src",
		"test": "nyc mocha --harmony --require esm scripts/tests.config.js --recursive tests",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"build": "rollup --config scripts/build.config.js",
		"start": "npm run build -- --watch",
		"prepare": "npm run build",
		"pretest": "npm run build"
	},
	"nyc": {
		"temp-directory": "./coverage/.nyc_output",
		"exclude": [
			"scripts",
			"tests"
		],
		"reporter": [
			"lcov",
			"text"
		]
	},
	"main": "./index.js",
	"module": "dist/dio.es.js",
	"types": "",
	"files": [
		"dist",
		"src",
		"pkg"
	],
	"unpkg": "",
	"jsdelivr": ""
}
